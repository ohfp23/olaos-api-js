    "chai": "^4.3.4",
    "np": "^9.2.0",
    "rollup": "^4.16.1",
    "rollup-plugin-dts": "^6.1.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@sin7y/ola-abi-wasm": "^0.0.4",
    "@sin7y/ola-crypto": "^0.0.4",
    "axios": "^1.6.7"
