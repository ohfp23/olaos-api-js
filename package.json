{
  "name": "olaos-api-js",
  "version": "0.0.16",
  "description": "JavaScript SDK for ola.",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "mocha --timeout 60000",
